@model IEnumerable<thechauffeurteam.Models.API.CabOffice>
@{
    ViewBag.Title = "AllCabOffices";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}



<!-- Header -->
<div class="header bgblue pb-8 pt-5 pt-md-8">
    <div class="container-fluid">
        <div class="header-body">
            <!-- Card stats -->
            <div class="row">
                <div class="col-xl-3 col-lg-6">
                    <a href="~/CabOffice/NewCabOffice">
                        <div class="card card-stats mb-4 mb-xl-0">
                            <div class="card-body">
                                <div class="row">
                                    <div class="col">
                                        <h5 class="card-title text-uppercase text-muted mb-0">new</h5>
                                        <span class="h2 font-weight-bold mb-0">@ViewBag.NewSum</span>
                                    </div>
                                    <div class="col-auto">
                                        <div class="icon icon-shape bg-info text-white rounded-circle shadow">
                                            <i class="fas fa-percent"></i>
                                        </div>
                                    </div>
                                </div>
                                <p class="mt-3 mb-0 text-muted text-sm">
                                    @*<span class="text-success mr-2"><i class="fas fa-arrow-up"></i> 12%</span>*@
                                    <span class="text-nowrap">See more detail</span>
                                </p>
                            </div>
                        </div>
                    </a>
                </div>
                <div class="col-xl-3 col-lg-6">
                    <a href="~/CabOffice/ApprovedCabOffice">
                        <div class="card card-stats mb-4 mb-xl-0">
                            <div class="card-body">
                                <div class="row">
                                    <div class="col">
                                        <h5 class="card-title text-uppercase text-muted mb-0">Approved</h5>
                                        <span class="h2 font-weight-bold mb-0">@ViewBag.ApprovedSum</span>
                                    </div>
                                    <div class="col-auto">
                                        <div class="icon icon-shape bg-warning text-white rounded-circle shadow">
                                            <i class="fas fa-chart-pie"></i>
                                        </div>
                                    </div>
                                </div>
                                <p class="mt-3 mb-0 text-muted text-sm">
                                    @*<span class="text-danger mr-2"><i class="fas fa-arrow-down"></i> 3.48%</span>*@
                                    <span class="text-nowrap">See more detail</span>
                                </p>
                            </div>
                        </div>
                    </a>
                </div>
                <div class="col-xl-3 col-lg-6">
                    <a href="~/CabOffice/RejectedCabOffice">
                        <div class="card card-stats mb-4 mb-xl-0">
                            <div class="card-body">
                                <div class="row">
                                    <div class="col">
                                        <h5 class="card-title text-uppercase text-muted mb-0">Rejected</h5>
                                        <span class="h2 font-weight-bold mb-0">@ViewBag.RejectedSum </span>
                                    </div>
                                    <div class="col-auto">
                                        <div class="icon icon-shape bg-danger text-white rounded-circle shadow">
                                            <i class="fas fa-chart-bar"></i>
                                        </div>
                                    </div>
                                </div>
                                <p class="mt-3 mb-0 text-muted text-sm">
                                    @*<span class="text-success mr-2"><i class="fa fa-arrow-up"></i> 3.48%</span>*@
                                    <span class="text-nowrap">See more detail</span>
                                </p>
                            </div>
                        </div>
                    </a>
                </div>
                <div class="col-xl-3 col-lg-6">
                    <a href="~/CabOffice/AllCabOffices">
                        <div class="card card-stats mb-4 mb-xl-0">
                            <div class="card-body">
                                <div class="row">
                                    <div class="col">
                                        <h5 class="card-title text-uppercase text-muted mb-0">All</h5>
                                        <span class="h2 font-weight-bold mb-0">@ViewBag.AllSum</span>
                                    </div>
                                    <div class="col-auto">
                                        <div class="icon icon-shape bg-yellow text-white rounded-circle shadow">
                                            <i class="fas fa-users"></i>
                                        </div>
                                    </div>
                                </div>
                                <p class="mt-3 mb-0 text-muted text-sm">
                                    @*<span class="text-warning mr-2"><i class="fas fa-arrow-down"></i> 1.10%</span>*@
                                    <span class="text-nowrap">See more detail</span>
                                </p>
                            </div>
                        </div>

                    </a>
                </div>
            </div>

        </div>
    </div>
</div>
<div class="container-fluid mt--8">
    <div class="row mt-5">

        <div class="col-xl-12 mb-5 mb-xl-0">
            <div class="card shadow">
                <div class="card-header border-0">
                    <div class="row align-items-center">
                        <div class="col">
                            <h3 class="mb-0">@ViewBag.txt</h3>
                        </div>
                        <div class="col text-right">
                            <a href="#!" class="btn btn-sm btn-primary">See all</a>
                        </div>
                    </div>
                </div>
                <div class="table-responsive">
                    <!-- Projects table -->
                    <table class="table align-items-center table-flush">
                        <thead class="bgblue">
                            <tr>
                                @*<th>status</th>*@
                                <th scope="col">Name</th>
                                <th scope="col">Email Address</th>
                                <th scope="col">Phone No</th>
                                <th scope="col">Fax</th>
                                <th scope="col">Postcode</th>
                                <th scope="col">Detail</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var rec in Model)
                            {
                                @Html.Partial("_CabOfficeNewAcceptReject", rec)
                            }

                        </tbody>
                    </table>
                </div>
            </div>
        </div>

    </div>
    <!-- Footer -->

    @Html.Partial("~/Views/Shared/_CabAndAdminFooter.cshtml")

</div>

