@model IEnumerable<thechauffeurteam.Models.job>

@{
    ViewBag.Title = "Booked";
}



<div style="width:50%;height:auto; background-color:#0026ff;border-radius:5px;margin-left:30%;margin-top:10%; margin-bottom:15%;">

    <h4 style="color:white;text-align:center;padding:10%;text-transform:capitalize;word-spacing:2px;">
        “Your job is booked. You will receive confirmation as soon as possible”. Thank You



    </h4>
    

</div>


@section scripts {
    <!--Script references. -->
    <!--The jQuery library is required and is referenced by default in _Layout.cshtml. -->
    <!--Reference the SignalR library. -->
    <script src="~/Scripts/jquery.signalR-2.4.0.js"></script>
    <!--Reference the autogenerated SignalR hub script. -->
    <script src="~/signalr/hubs"></script>
    <!--SignalR script to update the chat page and send messages.-->

    <script src="~/js/mycode.js"></script>
    }











